{
  "name": "agent-skills",
  "version": "1.0.0",
  "description": "Agent Skills for GitHub Copilot - TypeScript/Node.js implementation",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "create-ticket": "npx ts-node --esm skills/create-ticket/scripts/create-ticket.ts",
    "work-on-ticket": "npx ts-node --esm skills/work-on-ticket/scripts/start-work.ts",
    "fetch-ticket": "npx ts-node --esm skills/work-on-ticket/scripts/fetch-ticket.ts",
    "test-connection": "npx ts-node --esm skills/work-on-ticket/scripts/test-connection.ts"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
